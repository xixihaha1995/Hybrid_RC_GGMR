	doi = {10.1109/DDCLS49620.2020.9275082},
	abstract = {Data-driven soft sensor approach has been widely applied on real-time prediction and control of difficult-to-measure quality variables. Among these approaches, the Gaussian mixture regression (GMR) carries the potential of dealing with nonlinear and non-Gaussian industry problems, which has drawn increasing popularity and attentions in recent years. However, the fluctuation of raw materials, change of process environment, aging of instruments and other factors will have an effect on system performances over time. Hence, the lack of adaptive mechanism will make the GMR difficult to suit for time-varying processes and may cause large prediction errors. In order to model time-varying industrial processes and improve the adaptability of the conventional GMR, an adaptive soft sensor based on incremental Gaussian mixture regression (IGMR) is proposed in this paper. The incremental idea is integrated and an adaptive mechanism is added, which endow the proposed IGMR with the capability of adapting to new data in online environment. Compared to the moving window GMR (MWGMR) and the just-in-time learning GMR (JITLGMR), the feasibility and effectiveness of the proposed IGMR are verified both in a numerical simulation and a real-life industrial process experiment.},
	booktitle = {2020 {IEEE} 9th {Data} {Driven} {Control} and {Learning} {Systems} {Conference} ({DDCLS})},
	author = {Li, Deyang and Song, Zhihuan},
	month = nov,
	year = {2020},
	keywords = {Gaussian mixture model, Temperature measurement, Temperature sensors, Adaptation models, adaptive mechanism, Hydrogen, incremental, Process control, Production, Sensor phenomena and characterization, soft sensor, time-varying process},
	pages = {645--650},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\wulic\\Zotero\\storage\\UGZXS33D\\9275082.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\wulic\\Zotero\\storage\\LNWN3TKK\\Li and Song - 2020 - A Novel Incremental Gaussian Mixture Regression an.pdf:application/pdf},
}

