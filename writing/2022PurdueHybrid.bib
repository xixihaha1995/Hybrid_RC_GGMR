@article{sourbron_efficiently_2009,
	title = {Efficiently produced heat and cold is squandered by inappropriate control strategies: {A} case study},
	volume = {41},
	issn = {0378-7788},
	shorttitle = {Efficiently produced heat and cold is squandered by inappropriate control strategies},
	url = {https://www.sciencedirect.com/science/article/pii/S0378778809001145},
	doi = {10.1016/j.enbuild.2009.05.015},
	abstract = {The large thermal time constant of thermally activated building systems (TABS) hampers communication between the system's production and emission. Therefore, conventional building control strategies, typically using room temperature feedback, are unadapted to control thermal comfort efficiently. In this paper, measurement data and simulation results reveal that unadapted TABS control has a dramatic impact on overall energy performance. Measurements in a TABS building with room temperature feedback show the HVAC system switching between heating and cooling in a very short time frame. A simplified, generic room model is used to simulate, understand and evaluate this behaviour. For room temperature feedback control, only 45\% of the cold and 15\% of the heat produced actually controls room temperature. The remainder is stored in the TABS and exchanged between the heating and cooling system. Enlarging the heating-to-cooling set point band improves this ratio, while maintaining thermal comfort. On the other hand, night time operation control of the circulation pump, adapted to the TABS thermal time constant, eliminates this ‘unused’ energy completely. In this case, however, even with perfect heat gain forecasts, it is difficult to avoid room temperatures dropping below thermal comfort limits during initial office hours. Hence, a supplementary air-conditioning system seems inevitable.},
	language = {en},
	number = {10},
	urldate = {2022-04-11},
	journal = {Energy and Buildings},
	author = {Sourbron, M. and De Herdt, R. and Van Reet, T. and Van Passel, W. and Baelmans, M. and Helsen, L.},
	month = oct,
	year = {2009},
	keywords = {Simulation, Control strategy, High temperature cooling, Low temperature heating, measurements, Thermally activated building systems (TABS)},
	pages = {1091--1098},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\wulic\\Zotero\\storage\\YJDZ2XA4\\Sourbron et al. - 2009 - Efficiently produced heat and cold is squandered b.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\wulic\\Zotero\\storage\\SEAMRB3J\\S0378778809001145.html:text/html},
