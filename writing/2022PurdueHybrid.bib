	keywords = {Birds, Educational institutions, Genetic algorithms, Particle swarm optimization, Collaboration, Computational modeling, Evolutionary computation, Genetic mutations, Genetic programming, Nonlinear equations},
	pages = {69--73},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\wulic\\Zotero\\storage\\IPTNFZHM\\699146.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\wulic\\Zotero\\storage\\KUE8I33T\\Shi and Eberhart - 1998 - A modified particle swarm optimizer.pdf:application/pdf},
